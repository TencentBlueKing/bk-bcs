{
  "files.eol": "\n",
  "editor.tabSize": 2,
  "editor.codeActionsOnSave": [
    "source.fixAll.eslint"
  ],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "vue"
  ],
  "bk-code-ai.enable": false,
  "vue.codeActions.enabled": false,
  "i18n-ally.localesPaths": [
    "src/i18n"
  ],
}