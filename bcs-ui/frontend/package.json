{
  "name": "paas-bcs-webfe",
  "version": "1.0.0",
  "description": "Project description",
  "author": "bkfe",
  "scripts": {
    "dll": "node -v && rimraf static/lib* && node build/build-dll.js",
    "check-dll": "node build/check-dll.js",
    "dev": "better-npm-run dev",
    "dev:ee": "better-npm-run dev:ee",
    "build:analyzer": "better-npm-run build:analyzer",
    "clean:ee": "rimraf dist/ee",
    "clean:ce": "rimraf dist/ce",
    "clean:ieod": "rimraf dist/ieod",
    "clean:hard-source-cache": "rimraf .hard-source-cache",
    "build:ce": "better-npm-run build:ce",
    "build:ee": "better-npm-run build:ee",
    "build:ieod": "better-npm-run build:ieod"
  },
  "betterScripts": {
    "dev": {
      "command": "npm run check-dll && node build/dev-server.js",
      "env": {
        "NODE_ENV": "development",
        "VERSION": "ieod"
      }
    },
    "dev:ee": {
      "command": "npm run check-dll && node build/dev-server.js",
      "env": {
        "NODE_ENV": "development",
        "VERSION": "ee"
      }
    },
    "build:analyzer": {
      "command": "npm run clean:ieod && npm run dll && node build/build.js",
      "env": {
        "NODE_ENV": "production",
        "VERSION": "ieod",
        "npm_config_report": 1
      }
    },
    "build:ieod": {
      "command": "npm run clean:ieod && npm run dll && node --max-old-space-size=4096 build/build.js",
      "env": {
        "NODE_ENV": "production",
        "VERSION": "ieod",
        "CLEAN_HARD_SOURCE_CACHE": "1"
      }
    },
    "build:ce": {
      "command": "npm run clean:ce && npm run dll && node --max-old-space-size=4096 build/build.js && node build/replace-internal-info.js",
      "env": {
        "NODE_ENV": "production",
        "VERSION": "ce"
      }
    },
    "build:ee": {
      "command": "npm run clean:ee && npm run dll && node --max-old-space-size=4096 build/build.js && node build/replace-internal-info.js",
      "env": {
        "NODE_ENV": "production",
        "VERSION": "ee"
      }
    }
  },
