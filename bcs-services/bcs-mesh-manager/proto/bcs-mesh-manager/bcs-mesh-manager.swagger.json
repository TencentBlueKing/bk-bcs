        "parameters": [
          {
            "name": "projectCode",
            "description": "projectCode. 项目编码",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "meshID",
            "description": "meshID. 网格ID，可选过滤条件",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "name",
            "description": "name. 网格名称，可选过滤条件",
