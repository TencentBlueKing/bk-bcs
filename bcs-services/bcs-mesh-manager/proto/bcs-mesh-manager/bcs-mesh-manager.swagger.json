    "/meshmanager/v1/mesh/istio/config": {
      "get": {
        "summary": "获取当前开放的istio版本和配置信息",
        "operationId": "MeshManager_ListIstioConfig",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
