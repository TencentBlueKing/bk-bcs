            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "tags": [
          "MeshManager"
        ]
      }
    },
    "/meshmanager/v1/mesh/istio/detail/{meshID}": {
      "get": {
        "summary": "获取istio详情",
