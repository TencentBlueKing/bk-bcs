        "parameters": [
          {
            "name": "meshID",
            "description": "网格ID",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "projectID": {
                  "type": "string",
                  "description": "项目ID",
                  "title": "projectID"
                },
                "projectCode": {
                  "type": "string",
                  "description": "项目编码",
                  "title": "projectCode"
                },
                "name": {
                  "type": "string",
                  "description": "网格名称",
                  "title": "name"
                },
                "version": {
                  "type": "string",
                  "description": "使用的版本，例如 1.18",
                  "title": "version"
                },
                "controlPlaneMode": {
                  "type": "string",
                  "description": "安装模式[independent：独立控制面（默认）；hosting：托管控制面]",
                  "title": "controlPlaneMode"
                },
                "clusterMode": {
                  "type": "string",
                  "description": "多集群集群模式[primaryPemote：主从架构（默认）；multiPrimary：多主架构]",
                  "title": "clusterMode"
                },
                "description": {
                  "type": "string",
                  "description": "文字描述",
