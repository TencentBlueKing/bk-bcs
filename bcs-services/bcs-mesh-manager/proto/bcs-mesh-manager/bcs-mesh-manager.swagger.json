            "name": "meshID",
            "description": "网格ID",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "projectCode",
            "description": "projectCode. 项目编码",
            "in": "query",
            "required": false,
            "type": "string"
          }
