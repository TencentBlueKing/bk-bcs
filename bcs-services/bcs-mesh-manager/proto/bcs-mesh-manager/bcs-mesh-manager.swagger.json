            "required": false,
            "type": "string"
          },
          {
            "name": "page",
            "description": "page. 页码",
            "in": "query",
            "required": false,
            "type": "integer",
            "format": "int32"
          },
          {
            "name": "pageSize",
            "description": "pageSize. 每页数量",
            "in": "query",
            "required": false,
            "type": "integer",
            "format": "int32"
